record(calcout, $(P)angl) {
  field(DESC, "angle")
  field(SCAN, ".1 second")
  field(INPA, "icepap:m1.RBV")
  field(CALC, "A%360")
  field(PREC, 4)
}

record(calcout, $(P)time) {
  field(DESC, "angle")
  field(SCAN, ".1 second")
  field(VAL, 0)
  field(INPA, "time")
  field(CALC, "A+1")
  field(PREC, 1)
}
